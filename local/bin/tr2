#!/bin/bash

TEST_URL=spn0006::R/home1/irteam/user/kisow/package

if [[ "$1" == "" ]]
then
	echo "`basename $0` nexus_pkg"
	exit
fi

envoke()
{
	echo "$@"
	$@
}

echo "--------------------------------------------------------------------------------------------------------------\n"
envoke rsync -avz $1 $TEST_URL
echo "--------------------------------------------------------------------------------------------------------------\n"
envoke rsync -avz $TEST_URL/`basename $1`
echo "--------------------------------------------------------------------------------------------------------------\n"

# vim: enc=utf8 fdm=marker cms=#%s
