#!/bin/bash

HOTFIX_URL=sss::R/naver/pkgs/nexus

if [[ "$1" == "" ]]
then
	echo "`basename $0` nexus_pkg"
	exit
fi

envoke()
{
	echo "$@"
	$@
}

echo "--------------------------------------------------------------------------------------------------------------\n"
envoke rsync -avz $1 $HOTFIX_URL
echo "--------------------------------------------------------------------------------------------------------------\n"
envoke rsync -avz $HOTFIX_URL/`basename $1`
echo "--------------------------------------------------------------------------------------------------------------\n"

# vim: enc=utf8 fdm=marker cms=#%s
